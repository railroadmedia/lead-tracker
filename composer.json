{
  "name": "railroad/lead-tracker",
  "type": "library",
  "description": "Tracks email form submissions and marketing data for visitor origins.",
  "keywords": [
    "tracker",
    "lead"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Caleb Favor",
      "email": "caleb@drumeo.com",
      "role": "Developer"
    }
  ],
  "prefer-stable": true,
  "require": {
    "php": "^8.1",
    "laravel/framework": "^9.0",
    "doctrine/dbal": "^3.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.10",
    "phpunit/php-code-coverage": "^9.2.10",
    "orchestra/testbench": "^7.4",
    "dms/phpunit-arraysubset-asserts": "^0.4.0"
  },
  "autoload": {
    "psr-4": {
      "Railroad\\LeadTracker\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\LeadTracker\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "kylekatarnls/update-helper": true
    }
  }
}
